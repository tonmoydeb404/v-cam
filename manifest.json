{
  "manifest_version": 3,
  "name": "vCam",
  "version": "1.0",
  "description": "Configure Your Webcam",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}